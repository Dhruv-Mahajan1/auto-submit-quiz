{
  "manifest_version": 3,
  "name": "basic-chrome",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  },
  "content_scripts": [
    {
      "matches": [
        "https://docs.google.com/forms/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "alarms",
    "tabs",
    "activeTab"
  ]
}